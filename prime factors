
int prime_foctor[100000];
void prime_factors(int n){
    if(n%2==0){
        cout<<2<<ln;
        while(n%2==0) n/=2;
    }
    for(int i = 3;i<=sqrt(n);i+=2){
        if(n%i==0){
            cout<<i<<ln;
            while(n%i==0)n/=i;
        }
    }
    if(n>2) cout<<n;
    
}

void solve(){
  int n;
  cin>>n;
  prime_factors(n);
  
  
} 
